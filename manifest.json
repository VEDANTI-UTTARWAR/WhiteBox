{
  
  "name": "Box",
  "version": "0.1",
  "description": "Box",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"   
  },
  "permissions": [
        "tabs",
        "http://*/*",
        "https://*/*",
        "<all_urls>"
    ],
  "content_scripts": [
    {
      "js": ["content.js"],
      "css": ["content.css"],
      "matches": ["https://*.com/*"]
    }
  ]
}
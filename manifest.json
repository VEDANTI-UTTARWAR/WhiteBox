{
  "manifest_version": 2,

  "name": "White Box",
  "description": "Place the white box wherever we want",
  "version": "0.1",
  "author": "Vedanti Uttarwar",

  "browser_action": {
    "default_icon": {
      
    }
  },
  "permissions": [
        "tabs",
        "http://*/*",
        "https://*/*",
        "<all_urls>"
    ],
  "background": {
    "page": "box.html",
    "persistent": false           //we dont want to keep background page always open
  }
}